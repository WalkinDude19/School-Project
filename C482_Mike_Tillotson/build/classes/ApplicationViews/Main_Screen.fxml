<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="scr_Main_Screen" prefHeight="387.0" prefWidth="860.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ApplicationViews.Main_Screen_Controller">
    <children>
        <TableView fx:id="parts_table" layoutX="14.0" layoutY="112.0" prefHeight="175.0" prefWidth="400.0">
            <columns>
                <TableColumn fx:id="parts_ID_Column" prefWidth="100.0" text="ID" />
                <TableColumn fx:id="parts_Name_Column" prefWidth="100.0" text="Name" />
                <TableColumn fx:id="parts_Inventory_Column" prefWidth="100.0" text="Inv" />
                <TableColumn fx:id="parts_Price_Column" prefWidth="100.0" text="Price" />
            </columns>
        </TableView>
        <TableView fx:id="products_table" layoutX="435.0" layoutY="112.0" prefHeight="175.0" prefWidth="400.0">
            <columns>
                <TableColumn fx:id="products_ID_Column" prefWidth="100.0" text="ID" />
                <TableColumn fx:id="products_Name_Column" prefWidth="100.0" text="Name" />
                <TableColumn fx:id="products_Inventory_Column" prefWidth="100.0" text="Inv" />
                <TableColumn fx:id="products_Price_Column" prefWidth="100.0" text="Price" />
            </columns>
        </TableView>
        <Label fx:id="lbl_Parts" layoutX="30.0" layoutY="78.0" text="Parts" textFill="#2d029a">
            <font>
                <Font name="Times New Roman Bold" size="18.0" />
            </font>
        </Label>
        <Label fx:id="lbl_Products" layoutX="450.0" layoutY="78.0" text="Products" textFill="#2d029a">
            <font>
                <Font name="Times New Roman Bold" size="18.0" />
            </font>
        </Label>
        <Label fx:id="lbl_Main_Page" layoutX="20.0" layoutY="21.0" text="Inventory Management System" textFill="#2d029a">
            <font>
                <Font name="Times New Roman Bold" size="18.0" />
            </font>
        </Label>
        <Button fx:id="btn_Search_Parts" layoutX="209.0" layoutY="76.0" mnemonicParsing="false" onAction="#handle_Parts_Search" prefHeight="25.0" prefWidth="52.0" text="Search" />
        <TextField fx:id="txt_Search_Parts" layoutX="275.0" layoutY="76.0" prefHeight="25.0" prefWidth="140.0" />
        <Button fx:id="btn_Search_Products" layoutX="614.0" layoutY="76.0" mnemonicParsing="false" onAction="#handle_Products_Search" text="Search" />
        <TextField fx:id="txt_Search_Products" layoutX="696.0" layoutY="76.0" prefHeight="25.0" prefWidth="140.0" />
        <Button fx:id="btn_Add_Parts" layoutX="71.0" layoutY="298.0" mnemonicParsing="false" onAction="#handle_Add_Parts_Screen" prefHeight="37.0" prefWidth="89.0" text="Add" />
        <Button fx:id="btn_Modify_Parts" layoutX="173.0" layoutY="298.0" mnemonicParsing="false" onAction="#handle_Modify_Parts_Screen" prefHeight="37.0" prefWidth="89.0" text="Modify" />
        <Button fx:id="btn_Delete_Parts" layoutX="275.0" layoutY="298.0" mnemonicParsing="false" onAction="#handle_Parts_Delete_Screen" prefHeight="37.0" prefWidth="89.0" text="Delete" />
        <Button fx:id="btn_Add_Products" layoutX="475.0" layoutY="298.0" mnemonicParsing="false" onAction="#handle_Add_Products_Screen" prefHeight="37.0" prefWidth="89.0" text="Add" />
        <Button fx:id="btn_Modify_Products" layoutX="577.0" layoutY="298.0" mnemonicParsing="false" onAction="#handle_Modify_Products_Screen" prefHeight="37.0" prefWidth="89.0" text="Modify" />
        <Button fx:id="btn_Delete_Products" layoutX="677.0" layoutY="298.0" mnemonicParsing="false" onAction="#handle_Products_Delete" prefHeight="37.0" prefWidth="89.0" text="Delete" />
        <Button fx:id="btn_Exit" layoutX="776.0" layoutY="335.0" mnemonicParsing="false" onAction="#handle_Exit" prefHeight="25.0" prefWidth="60.0" text="Exit" />
